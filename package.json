{
  "name": "movers_v3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/setup-pdfkit.js",
    "dev": "next dev --port 3001",
    "prebuild": "node scripts/update-build-info.js && node scripts/setup-pdfkit.js",
    "build": "next build",
    "start": "node server.js",
    "start:next": "next start",
    "postinstall": "node scripts/setup-pdfkit.js || true",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup:env": "node setup-env.js",
    "check:env": "node -e \"console.log('OpenAI:', process.env.OPENAI_API_KEY ? '✅' : '❌'); console.log('Claude:', process.env.CLAUDE_API_KEY ? '✅' : '❌'); console.log('DB:', process.env.DATABASE_URL ? '✅' : '❌');\"",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:reset": "npx prisma db push --force-reset"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@prisma/client": "^6.16.3",
    "@types/pdfkit": "^0.17.3",
    "framer-motion": "^12.23.22",
    "next": "15.5.4",
    "openai": "^5.23.0",
    "pdfkit": "^0.17.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^30.2.0",
    "prisma": "^6.16.3",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "typescript": "^5"
  }
}
